class Point{
float x;
float y;
float distance(Point b)
{
	float dx;
	float dy;
	dx=b.x-x;
	dy=b.y-y;
	return (sqrt(dx*dx+dy*dy));
	
};
};

class Vector
{
	Point a;
	Point b;
	float length()
	{	
		return (a.dis(b));
	};
};
program
{
	Vector v;
	Point p;
	float l;
	float n;
	v.a.x=0;
	v.a.y=0;
	v.b.x=0;
	v.b.y=1;
	l= v.length();
	n=v.a.distanc(p);
};


float sqrt (float c)
{
	
};

float ef(float l, float r,float c)
{
	float min;
	if (r-l<=0.000001)
	then
	return (l);
	else{};
	min=(l+r)/2;
	if (c<min)
	then return (ef(l,min,c));
	else
	return (ef(min,r,c));;
	
};