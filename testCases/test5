class iDriver{
	int add(int a,int b)
	{
		return (a+b);
	};
	
	int addArray(int a[100],int b[100],int n)
	{
		int c[100];
		for (int i=0;i<n;i=i+1)
		{
			c[i]=a[i]+b[i];
		};
		for (int i=0;i<n;i=i+1)
		put(c[i]);;
	};
};

class fDriver{
	float add(int a,int b)
	{
		float aa;
		aa = a;
		return (aa+b);
	};
	
	float addArray(float a[100],float b[100],int n)
	{
		float c[100];
		for (int i=0;i<n;i=i+1)
		{
			c[i]=a[i]+b[i];
		};
		for (int i=0;i<n;i=i+1)
		put(c[i]);;
	};
};

program{
	int a[110];
	int b[100];
	float c[100];
	float d[100];
	int r1;
	int r2;
	int r3;
	int r4;
	iDriver dr;
	fDriver fr;
	r1=output1(dr.add(a[0],b[0]));
	r2=output2(fr.add(c[0],d[0]));
	r3=dr.addArray(a,b,100);
	r4=fr.addArray(c,d,100);
	
};

int output1(int k) //circular function call
{
	int r;
	r=output2(k);
	return (k);
};

float output2 (float k)
{	
	int r;
	r=output1(k);
	return (k);
};